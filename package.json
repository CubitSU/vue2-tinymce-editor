{
  "name": "vue2-tinymce-editor",
  "version": "0.0.1",
  "description": "A TinyMCE WYSIWYG editor component for your Vue products",
  "author": {
    "name": "Alex Beshine",
    "email": "alex31n@yahoo.com"
  },
  "license": "MIT",
  "main": "dist/vue2-tinymce-editor.ssr.js",
  "browser": "dist/vue2-tinymce-editor.esm.js",
  "module": "dist/vue2-tinymce-editor.esm.js",
  "unpkg": "dist/vue2-tinymce-editor.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --bundleConfigAsCjs",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs --bundleConfigAsCjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --bundleConfigAsCjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --bundleConfigAsCjs"
  },
  "dependencies": {
    "tinymce": "^5.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/compiler-sfc": "^3.1.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^4.21.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "vue",
    "vue-component",
    "vue2",
    "html editor",
    "text editor",
    "tinymce",
    "tinymce vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alex31n/vue2-tinymce-editor.git"
  }
}
